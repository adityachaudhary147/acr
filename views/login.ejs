<%- include('./partials/header'); -%>

<div class ="container">
    <h1 style ="text-align: center; width:30%; margin:30px auto; ">Login</h1>
    <div style ="width:30%; margin:0 auto; ">
        <form action="/login" method="POST">
            <div class="form-group">
                <input class ="form-control" type="text" name ="username" placeholder="Faculty ID" required>
            </div>
            <div class="form-group">
                <input class ="form-control" type="password" name ="password" placeholder="password" required>
            </div>
            <div class="form-group">
                <button class="btn btn-md btn-primary btn-block" >Submit</button>
            </div>
        </form>
      
    </div>
</div>

<%- include("./partials/footer"); -%>